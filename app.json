{
  "expo": {
    "name": "WildWood",
    "slug": "ww-app",
    "version": "2.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.waltkowalski.wwapp",
      "permissions": [
        "NOTIFICATIONS",
        "USER_FACING_NOTIFICATIONS",
        "LOCATION",
        "CAMERA",
        "AUDIO_RECORDING",
        "CAMERA_ROLL",
        "CALENDAR",
        "REMINDERS",
        "SYSTEM_BRIGHTNESS",
        "MOTION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
